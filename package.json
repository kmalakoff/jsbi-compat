{
  "name": "jsbi-compat",
  "version": "0.2.0",
  "description": "Provides a jsbi interface on top of native BigInt and provides a fallback to jsbi library when BigInt is not available. Adds isBigInt function for cross-implementation type checking",
  "keywords": [
    "BigInt",
    "big.js",
    "compatibility",
    "compat"
  ],
  "homepage": "https://github.com/kmalakoff/jsbi-compat",
  "repository": {
    "type": "git",
    "url": "git@github.com:kmalakoff/jsbi-compat.git"
  },
  "license": "MIT",
  "author": "Kevin Malakoff <kmalakoff@gmail.com> (https://github.com/kmalakoff)",
  "main": "index.js",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint .",
    "prepublishOnly": "dtd \"npm run lint\" \"depcheck\"",
    "test": "mocha-compat test/spec/**/*.test.js"
  },
  "dependencies": {
    "char-code-at": "0.0.1",
    "clz32": "^1.0.1",
    "imul": "^1.0.1",
    "jsbi": "^3.1.5",
    "max-safe-integer": "^2.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "depcheck": "^1.4.2",
    "dis-dat": "^0.1.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jsbi-tests": "^3.1.2",
    "mocha-compat": "^3.5.5",
    "mock-require-lazy": "^1.0.0",
    "prettier": "^2.3.2"
  },
  "engines": {
    "node": ">=0.8.0"
  }
}
