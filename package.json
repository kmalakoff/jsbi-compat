{
  "name": "jsbi-compat",
  "version": "1.0.2",
  "description": "Provides a jsbi interface on top of native BigInt and provides a fallback to jsbi library when BigInt is not available. Adds isBigInt function for cross-implementation type checking",
  "keywords": [
    "BigInt",
    "big.js",
    "compatibility",
    "compat"
  ],
  "homepage": "https://github.com/kmalakoff/jsbi-compat",
  "repository": {
    "type": "git",
    "url": "git@github.com:kmalakoff/jsbi-compat.git"
  },
  "license": "MIT",
  "author": "Kevin Malakoff <kmalakoff@gmail.com> (https://github.com/kmalakoff)",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsds build",
    "format": "biome check --write --unsafe",
    "prepublishOnly": "tsds validate",
    "test": "tsds test:node --no-timeouts",
    "test:engines": "nvu engines tsds test:node --no-timeouts",
    "version": "tsds version"
  },
  "dependencies": {
    "char-code-at": "*",
    "clz32": "*",
    "imul": "*",
    "jsbi": "*",
    "max-safe-integer": "*",
    "setprototypeof": "*"
  },
  "devDependencies": {
    "@biomejs/biome": "*",
    "@types/mocha": "*",
    "@types/node": "*",
    "jsbi-tests": "*",
    "mock-require-lazy": "*",
    "node-version-use": "*",
    "ts-dev-stack": "*"
  },
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index.ts"
  }
}
