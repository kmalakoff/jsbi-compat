{
  "name": "jsbi-compat",
  "version": "1.0.7",
  "description": "Provides a jsbi interface on top of native BigInt and provides a fallback to jsbi library when BigInt is not available. Adds isBigInt function for cross-implementation type checking",
  "keywords": [
    "BigInt",
    "big.js",
    "compatibility",
    "compat"
  ],
  "homepage": "https://github.com/kmalakoff/jsbi-compat",
  "repository": {
    "type": "git",
    "url": "git@github.com:kmalakoff/jsbi-compat.git"
  },
  "license": "MIT",
  "author": "Kevin Malakoff <kmalakoff@gmail.com> (https://github.com/kmalakoff)",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.js",
  "source": "src/index.ts",
  "types": "dist/cjs/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsds build",
    "format": "tsds format",
    "prepublishOnly": "tsds validate",
    "test": "tsds test:node --no-timeouts",
    "test:engines": "nvu engines tsds test:node --no-timeouts",
    "version": "tsds version"
  },
  "dependencies": {
    "char-code-at": "^0.0.1",
    "clz32": "^2.0.0",
    "imul": "^2.0.0",
    "jsbi": "^4.3.2",
    "max-safe-integer": "^2.0.0",
    "setprototypeof": "^1.2.0"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^25.0.1",
    "jsbi-tests": "^3.1.2",
    "mock-require-lazy": "^2.0.11",
    "node-version-use": "^2.1.5",
    "ts-dev-stack": "^1.21.3",
    "tsds-config": "^0.2.1"
  },
  "engines": {
    "node": ">=0.8"
  }
}
