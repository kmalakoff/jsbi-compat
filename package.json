{
  "name": "jsbi-compat",
  "version": "0.2.2",
  "description": "Provides a jsbi interface on top of native BigInt and provides a fallback to jsbi library when BigInt is not available. Adds isBigInt function for cross-implementation type checking",
  "keywords": [
    "BigInt",
    "big.js",
    "compatibility",
    "compat"
  ],
  "homepage": "https://github.com/kmalakoff/jsbi-compat",
  "repository": {
    "type": "git",
    "url": "git@github.com:kmalakoff/jsbi-compat.git"
  },
  "license": "MIT",
  "author": "Kevin Malakoff <kmalakoff@gmail.com> (https://github.com/kmalakoff)",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "",
    "format": "prettier --write .",
    "lint": "eslint .",
    "prepublishOnly": "npm run lint && depcheck",
    "test": "mocha-compat test/spec/**/*.test.js",
    "test:engines": "nvu engines npm test"
  },
  "dependencies": {
    "char-code-at": "0.0.1",
    "clz32": "^1.0.1",
    "imul": "^1.0.1",
    "jsbi": "^4.3.0",
    "max-safe-integer": "^2.0.0",
    "setprototypeof": "^1.2.0"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^5.33.0",
    "depcheck": "^1.4.3",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-promise": "^6.0.0",
    "jsbi-tests": "^3.1.2",
    "mocha-compat": "^3.5.5",
    "mock-require-lazy": "^1.0.1",
    "prettier": "^2.7.1"
  },
  "engines": {
    "node": ">=0.8"
  }
}
